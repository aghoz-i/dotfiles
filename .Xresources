URxvt*cursorUnderline: false
URxvt*scrollBar: false
!URxvt*internalBorder: 15
!URxvt*externalBorder: 5
URxvt*buffered: true
!URxvt*letterSpace: 0
URxvt*inheritPixmap: false
URxvt.termName: xterm-256color
!URxvt.scrollBar: false
!URxvt.transparent: true
!URxvt.shading: 20
URxvt*blurRadius: 5

!URxvt.tintColor: #172A47
!URxvt.inheritPixmap: true
URxvt.depth: 32

URxvt.perl-ext-common: default,clipboard,zoom
URxvt.keysym.Control-Shift-V: perl:clipboard:paste
URxvt.keysym.Control-Shift-C: perl:clipboard:copy
URxvt.keysym.C-equal: perl:zoom:in
URxvt.keysym.C-minus: perl:zoom:out

URxvt.iso14755: False


!Fonts
!Literation Mono Powerline
URxvt.font: xft:Menlo for Powerline:pixelsize=14,style=regular
!URxvt.font: xft:Ubuntu Mono:pixelsize=17,style=regular
URxvt.boldFont: xft:Menlo for Powerline:pixelsize=14,style=bold
!URxvt.boldFont: xft:Ubuntu Mono:pixelsize=17,style=bold
URxvt.letterSpace: -1


Xft.dpi: 90
Xft.antialias: true
Xft.font: xft:Noto Sans:pixelsize=10,style=regular
Xft.boldFont: xft:Noto Sans:pixelsize=10,style=bold

!Xft.rbga: none
Xft.hintstyle: hintfull
Xft.hinting: true

*.internalBorder: 13


